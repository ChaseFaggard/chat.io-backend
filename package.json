{
  "name": "socketio-server",
  "version": "1.0.0",
  "description": "A multiplayer game socketio server",
  "main": "dist/server/src/server.js",
  "scripts": {
    "tsc": "./node_modules/typescript/bin/tsc",
    "postinstall": "npm run tsc",
    "start": "node .",
    "dev:build": "tsc -w",
    "dev:run": "nodemon ./dist/server/src/server.js",
    "dev": "concurrently npm:dev:*",
    "deploy": "echo \"Pushing all changes to main with comment '${npm_config_comment}'\" && git add . && git commit -m \"${npm_config_comment}\" && git push origin main",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Chase Faggard",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/socket.io": "^3.0.2",
    "express": "^4.17.2",
    "socket.io": "^4.4.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.5",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
